{
    "env": {
      "browser": true,
      "jquery": true
    },
    "extends": "eslint:recommended",
    "globals": {
    },
    "parserOptions": {
        "ecmaVersion": 2015
    },
    "rules": {
      "no-console": "off",
      "no-extra-semi": "warn",
      "no-undef": "warn",
      "space-before-blocks": ["warn", { "functions": "always" }],
      "no-unused-vars": 1
    }
  }